{
  "name": "guitar",
  "version": "1.0.0",
  "description": "Campfire songs",
  "main": "app.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/srcServer.js",
    "build:html": "babel-node tools/buildHtml.js",
    "build:dev": "webpack --config ./webpack.config.dev.js",
    "build:prod": "webpack --config ./webpack.config.prod.js",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "bounce": "rm -rf node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travishoki/guitar-music.git"
  },
  "author": "Travis Hoki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/travishoki/guitar-music/issues"
  },
  "homepage": "https://github.com/travishoki/guitar-music#readme",
  "dependencies": {
    "npm-run-all": "1.8.0",
    "react": "^16.14.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^11.1.3",
    "open": "0.0.5",
    "prop-types": "^15.5.10",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "2.10.0"
